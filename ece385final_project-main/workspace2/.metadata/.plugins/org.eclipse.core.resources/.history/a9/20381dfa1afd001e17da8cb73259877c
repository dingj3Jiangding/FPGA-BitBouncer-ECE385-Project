					   0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,
					   0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,
					   0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,
					   0x99, 0x99, 0x99, 0x99, 0x99, 0x99, 0x99, 0x90, 0x00,
					   0x00, 0x00, 0x00, 0x00};
zombie_slot_t zombie_map[4][3];
uint8_t       zombie_cnt = 0;

void clear_zombie(zombie_t* zombie_ptr, BRAM_t* hdmi_ctrl){
	for(uint8_t x = 0; x < (ZOMBIE_WIDTH/2); x++){
		for(uint8_t y = 0; y < ZOMBIE_HEIGHT; y++){
			uint8_t pixel = zombie_img[y * (ZOMBIE_WIDTH/2) + x];
			pixel = ((pixel & 0x0f) << 4) | ((pixel & 0xf0) >> 4);
			if(pixel!=0) hdmi_ctrl->VRAM[(zombie_ptr->pos_y + y - ZOMBIE_HEIGHT) * 320 + zombie_ptr->pos_x/2 + x] = 0;
		}
	}
}

void draw_zombie(zombie_t* zombie_ptr, BRAM_t* hdmi_ctrl){
	for(uint8_t x = 0; x < (ZOMBIE_WIDTH/2); x++){
		for(uint8_t y = 0; y < ZOMBIE_HEIGHT; y++){
			uint8_t pixel = zombie_img[y * (ZOMBIE_WIDTH/2) + x];
			pixel = ((pixel & 0x0f) << 4) | ((pixel & 0xf0) >> 4);
			if(pixel!=0) hdmi_ctrl->VRAM[(zombie_ptr->pos_y + y - ZOMBIE_HEIGHT) * 320 + zombie_ptr->pos_x/2 + x] = pixel;
		}
	}
}

void init_zombie_map(void){
	for(uint8_t i=0;i<4;i++){
		for(uint8_t j=0;j<3;j++){
			zombie_map[i][j].is_present = 0;
			zombie_cnt = 0;
		}
	}
}

void generate_zombie(zombie_t zombie_inst){
	uint8_t row = random_row();
	for(uint8_t i=0;i<3;i++){
		if(zombie_map[row][i].is_present == 0){
			zombie_map[row][i].is_present = 1;
			zombie_cnt++;
			zombie_map[row][i].zombie_inst = zombie_inst;
		}
	}
}

void draw_zombie_map(BRAM_t* hdmi_ctrl){
	for(uint8_t i=0;i<4;i++){
		for(uint8_t j=0;j<3;j++){
			if(zombie_map[i][j].is_present == 1){
				draw_zombie();
			}
		}
	}
}


