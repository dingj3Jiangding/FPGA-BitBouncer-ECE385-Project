					   0x44, 0x44, 0x44, 0x44, 0x44, 0x44, 0x49, 0x99, 0x99,
					   0x99, 0x99, 0x99, 0x99, 0x00, 0x00, 0x00, 0x00, 0x00,
					   0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,
					   0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x09, 0x99,
					   0x99, 0x94, 0x44, 0x44, 0x44, 0x44, 0x49, 0x99, 0x99,
					   0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,
					   0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,
					   0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,
					   0x99, 0x99, 0x99, 0x99, 0x99, 0x99, 0x99, 0x90, 0x00,
					   0x00, 0x00, 0x00, 0x00};
zombie_slot_t zombie_map[4][3];
uint8_t       zombie_cnt = 0;

void clear_zombie(zombie_t* zombie_ptr, BRAM_t* hdmi_ctrl){
	for(uint8_t x = 0; x < (ZOMBIE_WIDTH/2); x++){
		for(uint8_t y = 0; y < ZOMBIE_HEIGHT; y++){
			uint8_t pixel = zombie_img[y * (ZOMBIE_WIDTH/2) + x];
			pixel = ((pixel & 0x0f) << 4) | ((pixel & 0xf0) >> 4);
			if(pixel!=0) hdmi_ctrl->VRAM[(zombie_ptr->pos_y + y - ZOMBIE_HEIGHT) * 320 + zombie_ptr->pos_x/2 + x] = 0;
		}
	}
}

void draw_zombie(zombie_t* zombie_ptr, BRAM_t* hdmi_ctrl){
	for(uint8_t x = 0; x < (ZOMBIE_WIDTH/2); x++){
		for(uint8_t y = 0; y < ZOMBIE_HEIGHT; y++){
			uint8_t pixel = zombie_img[y * (ZOMBIE_WIDTH/2) + x];
			pixel = ((pixel & 0x0f) << 4) | ((pixel & 0xf0) >> 4);
			if(pixel!=0) hdmi_ctrl->VRAM[(zombie_ptr->pos_y + y - ZOMBIE_HEIGHT) * 320 + zombie_ptr->pos_x/2 + x] = pixel;
		}
	}
}

void init_zombie_map(void){
	for(uint8_t i=0;i<4;i++){
		for(uint8_t j=0;j<3;j++){
			zombie_map[i][j].is_present = 0;
			zombie_cnt = 0;
		}
	}
}

void generate_zombie(void){
	uint8_t row = random_row();
	for(uint8_t i=0;i<3;i++){
		if(zombie_map[row][i].is_present == 0){
			zombie_map[row][i].is_present = 1;
			zombie_cnt++;
			zombie_map[row][i].zombie_inst.pos_x = 580; //TODO: to be changed
			zombie_map[row][i].zombie_inst.pos_y = (row * SLOT_HEIGHT) + 155;
			break;
		}
	}
}

void update_zombie_map(BRAM_t* hdmi_ctrl){
	uint8_t grid_col;
	int rect_val;
	// Initialize that no slot is under attack
	for(uint8_t i=0; i< 32; i++){
		lawn_map[i].under_attack = 0;
	}

	for(uint8_t i=0;i<4;i++){
		for(uint8_t j=0;j<3;j++){
			if(zombie_map[i][j].is_present == 1){
				zombie_t zombie_inst = zombie_map[i][j].zombie_inst;
				clear_zombie(&zombie_inst,hdmi_ctrl);

				// Calculate rectified position
				rect_val = zombie_inst.pos_x - 35;
				// Set rectified value to 0 if smaller than 0
				if (rect_val < 0) rect_val = 0;
				// Calculate the gird column number
				grid_col = rect_val / SLOT_WIDTH;
				if (grid_col > 7) grid_col = 7;
				if(lawn_map[i*8+grid_col].plant_type == 0){
					zombie_inst.pos_x -= ZOMBIE_X_STEP;
				}else{
					// This slot is under attack
					lawn_map[i*8+grid_col].under_attack = 1;
				}

				zombie_map[i][j].zombie_inst = zombie_inst;
				draw_zombie(&zombie_inst,hdmi_ctrl);
			}
		}
	}
}

void draw_zombie_map(BRAM_t* hdmi_ctrl){
	for(uint8_t i=0;i<4;i++){
		for(uint8_t j=0;j<3;j++){
			if(zombie_map[i][j].is_present == 1){
				zombie_t zombie_inst = zombie_map[i][j].zombie_inst;
				draw_zombie(&zombie_inst,hdmi_ctrl);
			}
		}
	}
}

void clear_zombie_map(BRAM_t* hdmi_ctrl){
	for(uint8_t i=0;i<4;i++){
		for(uint8_t j=0;j<3;j++){
			if(zombie_map[i][j].is_present == 1){
				zombie_t zombie_inst = zombie_map[i][j].zombie_inst;
				clear_zombie(&zombie_inst,hdmi_ctrl);
			}
		}
	}
}

void update_zombie_pos(void){
	for(uint8_t i=0;i<4;i++){
		for(uint8_t j=0;j<3;j++){
			if(zombie_map[i][j].is_present == 1){
				zombie_map[i][j].zombie_inst.pos_x-= ZOMBIE_X_STEP;
			}
		}
	}
}

// return 1 if there is zombie on the row; esle return 0
uint8_t check_zombie(uint8_t row){
	for(uint8_t j=0;j<3;j++){
		if(zombie_map[row][j].is_present == 1){
			return 1;
		}
	}
	return 0;
}

